#include <iostream>

using namespace std;

int main()
{
    int i;
    double f;
    float f1;
    char s[30];
    
    printf(" pls input a interger \n");
    scanf("%d",&i);
    printf("your input interger= %d\n",i);
    
    printf(" pls input a float number \n");
    scanf("%lf",&f);
    printf("your input float number= %lf\n",f);
    
    printf(" pls input a string \n");
    scanf("%s",s);
    printf("your input string= %s\n",s);
    
    
    return 0;
}
